{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "parserOptions": {
       "parser": "babel-eslint"
   },
  "extends": [
      "airbnb-base",
      "plugin:vue/recommended"
  ],
  "rules": {
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "import/extensions": ["error", "always", {
      "js": "never",
      "vue": "never"
    }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js",".jsx",".vue"]
      }
    }
  }
}